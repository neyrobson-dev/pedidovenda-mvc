unit projetopedidovenda.model.entity.produto;

interface

type
  TProduto = class
  private
    FId: Integer;
    FDescricao: String;
    FPrecoVenda: Currency;
  public
    property Id: Integer read FId write FId;
    property Descricao: String read FDescricao write FDescricao;
    property FPrecoVenda: Currency;
  end;

implementation

end.
